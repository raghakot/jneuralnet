<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.apisupport.project</type>
    <configuration>
        <data xmlns="http://www.netbeans.org/ns/nb-module-project/3">
            <code-name-base>org.wrapper.jneuralnet</code-name-base>
            <suite-component/>
            <module-dependencies>
                <dependency>
                    <code-name-base>org.openide.util</code-name-base>
                    <build-prerequisite/>
                    <compile-dependency/>
                    <run-dependency>
                        <specification-version>7.22.1.1</specification-version>
                    </run-dependency>
                </dependency>
                <dependency>
                    <code-name-base>org.wrapper.swingx</code-name-base>
                    <build-prerequisite/>
                    <compile-dependency/>
                    <run-dependency>
                        <specification-version>1.0</specification-version>
                    </run-dependency>
                </dependency>
                <dependency>
                    <code-name-base>org.wrapper.xstream</code-name-base>
                    <build-prerequisite/>
                    <compile-dependency/>
                    <run-dependency>
                        <specification-version>1.0</specification-version>
                    </run-dependency>
                </dependency>
            </module-dependencies>
            <public-packages>
                <package>jneuralnet.core</package>
                <package>jneuralnet.core.activation</package>
                <package>jneuralnet.core.datastore</package>
                <package>jneuralnet.core.learning</package>
                <package>jneuralnet.core.learning.costfunction</package>
                <package>jneuralnet.core.preprocessor</package>
                <package>jneuralnet.core.training</package>
                <package>jneuralnet.util</package>
            </public-packages>
            <class-path-extension>
                <runtime-relative-path>ext/JNeuralNet.jar</runtime-relative-path>
                <binary-origin>release/modules/ext/JNeuralNet.jar</binary-origin>
            </class-path-extension>
        </data>
    </configuration>
</project>
